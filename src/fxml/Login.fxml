<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="anchorPane" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LoginController">
   <children>
      <Label fx:id="usernameLabel" alignment="CENTER" layoutX="14.0" layoutY="14.0" prefHeight="30.0" prefWidth="90.0" text="Username:" AnchorPane.leftAnchor="15.0" AnchorPane.topAnchor="15.0" />
      <Label fx:id="passwordLabel" alignment="CENTER" layoutX="17.0" layoutY="51.0" prefHeight="30.0" prefWidth="90.0" text="Password:" AnchorPane.leftAnchor="15.0" AnchorPane.topAnchor="60.0" />
      <TextField fx:id="usernameField" layoutX="181.0" layoutY="17.0" prefHeight="30.0" prefWidth="180.0" AnchorPane.leftAnchor="120.0" AnchorPane.rightAnchor="15.0" AnchorPane.topAnchor="15.0" />
      <PasswordField fx:id="passwordField" layoutX="183.0" layoutY="54.0" onAction="#login" prefHeight="30.0" prefWidth="180.0" AnchorPane.leftAnchor="120.0" AnchorPane.rightAnchor="15.0" AnchorPane.topAnchor="60.0" />
      <Button layoutX="220.0" layoutY="118.0" mnemonicParsing="false" onAction="#login" prefHeight="30.0" prefWidth="90.0" text="Login" AnchorPane.rightAnchor="15.0" AnchorPane.topAnchor="105.0" />
      <Button layoutX="210.0" layoutY="187.0" mnemonicParsing="false" onAction="#newUser" prefHeight="30.0" prefWidth="90.0" text="New User" AnchorPane.bottomAnchor="15.0" AnchorPane.rightAnchor="15.0" AnchorPane.topAnchor="155.0" />
   </children>
</AnchorPane>
